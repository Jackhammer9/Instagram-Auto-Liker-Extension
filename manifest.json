{
    "manifest_version": 3,
    "version": "1.0" ,
    "name": "Instagram like Bot",
    "action": {
        "default_popup": "index.html"
    },
    "permissions":[
        "tabs" , "scripting"
    ],
    "host_permissions": [
        "http://*.instagram.com/",
        "https://*.instagram.com/"
    ],
    "icons": { "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" }
}